<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Title</title>
</head>
<body>
<%= form_for @event, :url => event_path(@event), :method => :patch do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <%= f.label :description, "Description" %>
  <%= f.text_area :description %>
  <%= f.submit "Update" %>
<% end %>
</body>
</html>